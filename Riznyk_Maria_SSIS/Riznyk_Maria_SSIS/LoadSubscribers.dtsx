                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    S.SubscriberID, 
    S.FirstName, 
    S.LastName, 
    S.City, 
    S.BirthDate,
    ST.StatusName AS CurrentStatus,
    CASE 
        WHEN S.BirthDate IS NULL THEN 'Unknown'
        WHEN DATEDIFF(year, S.BirthDate, GETDATE()) &lt; 25 THEN 'Youth (&lt;25)'
        WHEN DATEDIFF(year, S.BirthDate, GETDATE()) &lt; 60 THEN 'Adult (25-60)'
        ELSE 'Senior (60+)'
    END AS AgeGroup
FROM dbo.Subscriber S
INNER JOIN ref.SubscriberStatus ST ON S.SubscriberStatusID = ST.SubscriberStatusID</property>

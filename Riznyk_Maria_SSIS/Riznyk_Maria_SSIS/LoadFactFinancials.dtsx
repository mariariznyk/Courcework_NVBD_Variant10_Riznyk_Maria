                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    P.PaymentID,
    I.SubscriberID,        
    P.PaymentMethodID,
    P.Amount,
    CAST(CONVERT(VARCHAR(8), P.PaymentDateTime, 112) AS INT) AS DateKey,
    SCG.ChannelGroupID AS Source_ChannelGroupID 
FROM dbo.Payment P
JOIN dbo.Invoice I ON P.InvoiceID = I.InvoiceID
JOIN dbo.SubscriberChannelGroup SCG ON I.SubscriberID = SCG.SubscriberID
WHERE I.SubscriberID IS NOT NULL</property>

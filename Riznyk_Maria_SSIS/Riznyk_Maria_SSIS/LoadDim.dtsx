                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    M.MovieID,
    M.MovieTitle,
    M.ReleaseYear,
    M.AgeRating,
    M.BasePrice, 
    ISNULL(G.GenreName, 'Unknown') AS GenreName
FROM dbo.Movie M
OUTER APPLY (
    SELECT TOP 1 GenreID 
    FROM dbo.MovieGenre MG 
    WHERE MG.MovieID = M.MovieID
) FirstGenre
LEFT JOIN ref.Genre G ON FirstGenre.GenreID = G.GenreID</property>
